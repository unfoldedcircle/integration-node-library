{
	"name": "uc-integration-api",
	"version": "0.0.7",
	"description": "NodeJS wrapper for the Unfolded Circle Integration API",
	"main": "index.js",
	"directories": {
		"example": "examples",
		"lib": "lib"
	},
	"dependencies": {
		"bonjour-service": "^1.2.1",
		"ws": "^8.18.0"
	},
	"devDependencies": {
		"ava": "^6.1.3",
		"prettier": "^3.3.3",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-config-semistandard": "^17.0.0",
		"eslint-plugin-prettier": "5.2.1",
		"eslint-plugin-promise": "^6.6.0"
	},
	"scripts": {
		"format": "prettier --write \"./{examples,lib,test}/**/*.js\" index.js",
		"lint": "prettier --check \"./{examples,lib,test}/**/*.js\" index.js && eslint . --ext .js,.jsx,.ts,.tsx",
		"test": "ava"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/unfoldedcircle/integration-node-library.git"
	},
	"keywords": [
		"uc",
		"unfoldedcircle",
		"remotetwo",
		"smartremote",
		"api",
		"integration"
	],
	"author": "Unfolded Circle",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/unfoldedcircle/integration-node-library/issues"
	},
	"homepage": "https://unfoldedcircle.com",
	"private": true,
	"engines": {
		"node": ">=16.18.0"
	}
}
